<h3>New Shelter</h3>
<%= link_to 'Shelters', "/shelters", method: :get %>
<%= link_to 'Pets', "/pets", method: :get %>

<p>*All Fields Required</p>
<%= form_tag '/shelters' do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %><br>
  <%= label_tag :address %>
  <%= text_field_tag :address %><br>
  <%= label_tag :city %>
  <%= text_field_tag :city %><br>
  <%= label_tag :state %>
  <%= text_field_tag :state %><br>
  <%= label_tag :zip %>
  <%= text_field_tag :zip %><br>

  <%= submit_tag 'Create Shelter' %>
<% end %>